{
  "rewrites": [
    {
      "source": "/mjkit-frontend/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/mjkit-frontend/([^/]+\\.[^/]+)$",
      "destination": "/$1"
    },
    {
      "source": "/mjkit-frontend/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/mjkit-frontend",
      "destination": "/index.html"
    },
    {
      "source": "/mjkit-frontend/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}